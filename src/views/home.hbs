<h1>Products</h1>
<div class="container">
    {{#each products}}
        <div class="product">
            <p class="title">{{title}}</p>
            <p class="description">{{description}}</p>
            <p class="price">Price: $ {{price}}</p>
            <p class="stock">Stock: {{stock}}</p>
        </div>
    {{/each}}
</div>